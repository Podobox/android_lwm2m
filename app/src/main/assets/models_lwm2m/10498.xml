<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>Discharge</Name>
        <Description1><![CDATA[This LWM2M object provides water discharge events for a water tank monitoring device.
]]></Description1>
        <ObjectID>10498</ObjectID>
        <ObjectURN>urn:oma:lwm2m:x:10498</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="0">
                <Name>Discharge Level</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units>m</Units>
                <Description><![CDATA[The water level to discharge to in metres for a water level discharge event]]></Description>
            </Item>
            <Item ID="1">
                <Name>Discharge Date Time Storm</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Time</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description><![CDATA[The date time for which a water level discharge event should occur]]></Description>
            </Item>
            <Item ID="2">
                <Name>Discharge Volume</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units>l</Units>
                <Description><![CDATA[The water volume to discharge in litres for a volume discharge event]]></Description>
            </Item>
            <Item ID="3">
                <Name>Discharge Date Time Volume</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Time</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description><![CDATA[The date time for which a volume discharge event should occur]]></Description>
            </Item>
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>

