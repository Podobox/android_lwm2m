<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>Manhole</Name>
        <Description1><![CDATA[This object provides site specific configuration for devices operating in a sewer manhole.]]></Description1>
        <ObjectID>10493</ObjectID>
        <ObjectURN>urn:oma:lwm2m:x:10493</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="0">
				<Name>Cover level</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The level of the manhole cover measured relative to a height datum eg. mAHD]]></Description>
			</Item>
            <Item ID="1">
				<Name>Sensor face level</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The level of the freespace distance sensor relative to a height datum eg. mAHD]]></Description>
			</Item>
            <Item ID="2">
				<Name>Invert level</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The level of the manhole invert measured relative to a height datum eg. mAHD]]></Description>
			</Item>
            <Item ID="3">
				<Name>Static object</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Multiple</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The level of a static object (eg. platform, ladder, etc.) in the manhole measured relative to a height datum eg. mAHD]]></Description>
			</Item>
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>

